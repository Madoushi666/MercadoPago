{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "MercadoPago.CheckoutAPI.Application.Services": "Information",
      "MercadoPago.CheckoutAPI.Application.Services.MercadoPago": "Information"
    }
  },
  "ConnectionStrings": {
    "StoreDB": "Data Source=SERVER;Database=DATABASE;Trusted_Connection=true;TrustServerCertificate=True;", // <<< Windows Authentication,
    //"StoreDB": "Data Source=SERVER;Database=DATABASE;User ID=USER;Password=PASSWORD;TrustServerCertificate=True;" // <<< SQL Server Authentication
  },
  "MercadoPago": {
    "UrlBase": "https://api.mercadopago.com/v1/",
    "AccessToken": "(ACCESS TOKEN)",
    "PublicKey": "(PUBLIC KEY)",
    "MaxRetriesHTTP": 3,
    "RetryDelayMilliseconds": 1000
  },
  "Jwt": {
    "Issuer": "(ISSUER)",
    "SecretKey": "(SECRET KEY)",
    "Expires": 10
  }
}
